{
  "name": "visualization",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development parcel src/index.html --open",
    "build": "NODE_ENV=production parcel build src/index.html",
    "predeploy": "npm run clean && npm run build",
    "clean": "gh-pages-clean",
    "deploy": "gh-pages -d dist",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yonastefera/d3charts.git"
  },
  "bugs": {
    "url": "https://github.com/yonastefera/d3charts/issues"
  },
  "homepage": "https://github.com/yonastefera/d3charts#readme",
  "lint-staged": {
    "*.{js, json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "postcss": {
    "modules": false,
    "plugins": {
      "autoprefixer": {
        "Browserslist": [
          ">1%",
          "last 4 versions",
          "Firefox ESR",
          "not ie < 9"
        ],
        "flexbox": "no-2009"
      }
    }
  },
  "dependencies": {
    "d3": "^5.16.0",
    "d3.js": "0.0.2-security",
    "gh-pages": "^3.1.0",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "eslint": "^7.7.0",
    "eslint-plugin-react": "^7.20.6",
    "node-sass": "^4.14.1",
    "babel-eslint": "^10.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "autoprefixer": "^9.8.6",
    "cssnano": "^4.1.10",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.12",
    "prettier": "^2.1.0"
  }
}
